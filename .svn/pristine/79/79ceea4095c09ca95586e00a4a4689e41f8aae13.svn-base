.plan-containers {
    background: #fff;
    ul {
        li.li-containers {
            width: 100%;
            height: 72px;
            position: relative;
            top: -3px;
            padding: 5px 8px;
            border-bottom: 5px solid #f2f2f2;
            &:last-child {
                border: none;
            }
          .li-top {
            span {
              font-size: 14px;
              color: #888;
            }
            span:nth-child(2){
              margin-left: 12px;
            }
            span.name {
              color: black;
            }
            span.kj{
              float: right;
            }
            span.time {
              color: red;
            }
          }
          .li-blow {
            margin-top: 12px;
            .onKj {
              color: forestgreen;
            }
            ul {
              display: flex;
              li {
                width: 20px;
                height: 20px;
                margin-right: 5px;
                text-align: center;
                border-radius: 50%;
                border: none;
                background: #EE0024;
                font-size: 12px;
                line-height: 19px;
                color: #fff;
              }
            }
          }
        }
    }
}
.am-drawer-content {
    background: #fff;
}
