.bar.bar-header {
    background: #EE0024;
  }
  .bar.bar-light .title{
    color: #fff;
  }
  .button .icon {
    color: #ffffff;
  }
  
  .von-input-wrapper .von-input {
    margin: 0;
  }
  .modal .bar.bar-header h1.title {
    color: #fff;
  }
.bar .title+.button:last-child {
  color: #fff;
}
.g-core-image-upload-btn {
  width: 50px;
  height: 70px;
  position: absolute !important;
  opacity: 0;
}
.tabs-striped.tabs-background-light .tabs {
  height: 38px;
}
.tabs {
  line-height: 34px;
}
.tabs-striped.tabs-color-positive .tab-item.tab-item-active, .tabs-striped.tabs-color-positive .tab-item.active, .tabs-striped.tabs-color-positive .tab-item.activated {
  font-size: 16px;  
  color: #EE0024;
  border: 0px solid #EE0024;
}
.tabs-striped.tabs-color-positive .tab-item {
  font-size: 16px;
  color: #444;
}
.tab-item {
    max-width: 207px;
}
.top-font {
    position: fixed; 
    top: 13px;
    left: 46%;
    z-index: 100;
    font-size: 18px;
    color: #fff;
}

.white-bg {
    width: 100%;
    height: 172px;
    position: fixed;
    left: 0;
    top: 0;
    z-index: -1;
    background: #fff;
}
.user-container {
    width: 100%;
    height: calc(100vh);
    overflow-y: scroll;
    // margin-top: 83px;
    background: #fff;
    .hangong {
        
        ul {
            padding-bottom: 60px;
            li {
                width: calc(100vw - 20px);
                min-height: 47px;
                margin: 0 auto;
                padding-top: 4px;
                display: flex;
                flex-wrap: wrap;
                border-bottom: 1px solid #e5e5e5;
                .exit-btn {
                    width: calc(100vw - 60px);
                    height: 44px;
                    margin: 0 auto;
                    border-radius: 22px;
                    text-align: center;
                    background: #E2E0E0;
                    p {
                        font-size: 16px;
                        line-height: 43px;
                        color: #444;
                    }
                }
                .li-left {
                    width: 70px;
                    height: auto;
                    margin-right: 20px;
                    text-align: right;
                    p {
                        font-size: 16px;
                        line-height: 35px;
                        color: #444;
                    }
                }
                img {
                    width: 64px;
                    height: 64px;
                    margin: 8px;
                    margin-right: 0px;
                }
                span {
                    font-size: 16px;
                    line-height: 39.5px;
                    color: #888;
                }
                input {
                    width: 70%;
                }
                input::-webkit-input-placeholder {
                    color: #888;
                }
            }
        }
        .red-btn {
            width: 88%;
            height: 44px;
            margin: 0 auto;
            border-radius: 22px;
            background: #EE0024;
            text-align: center;
            font-size: 16px;
            color: #fff;
            line-height: 45px;
        }
    }
}

.money-btn {
    width: 90px;
    height: 25px;
    position: absolute;
    top: 11px;
    right: 1%;
    text-align: center;
    border: 1px solid #EE0024;
    border-radius: 3px;
    p {
        font-size: 15px;
        line-height: 23px;
        color: #EE0024;
    }
}
.select-img {
    width: 60px;
    height: 60px;
    border: 4px solid #EE0024;
    border-radius: 50%;
    margin: 8px;
    margin-right: 0px;
}

.save-btn-center {
    width: 100%;
    height: 44px;
    position: fixed;
    left: 0;
    bottom: 0;
    text-align: center;
    color: #fff;
    font-size: 16px;
    line-height: 45px;
    background: #EE0024;
}
