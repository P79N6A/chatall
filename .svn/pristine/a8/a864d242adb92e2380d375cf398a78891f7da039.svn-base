.line-w{.rel;}
.line-w:after{.line-w-content;}
.line-w-content{content:'';position:absolute;left:0;bottom:0;right:auto;top:auto;height:1px;width:100%;background-color:#c8c7cc;display:block;z-index:15;-webkit-transform-origin:50% 100%;transform-origin:50% 100%; transform: scaleY(0.5); }
html.device-pixel-ratio-2 .line-w:after{-webkit-transform:scaleY(.5);transform:scaleY(.5);}
html.device-pixel-ratio-3 .line-w:after{-webkit-transform:scaleY(.33);transform:scaleY(.33);}
.line-h{.rel}
.line-h:after{.line-h-content;}
.line-h-content{content:'';position:absolute;bottom:0;top:0;height:100%;width:1px;background-color:@cBorder;display:block;z-index:15;-webkit-transform-origin:50% 100%;transform-origin:50% 100%; transform: scaleX(0.5); }
html.device-pixel-ratio-2 .line-h:after{-webkit-transform:scaleY(.5);transform:scaleY(.5);}
html.device-pixel-ratio-3 .line-h:after{-webkit-transform:scaleY(.33);transform:scaleY(.33);}

::-moz-placeholder{color:@c9;opacity:1}
:-ms-input-placeholder{color:@c9}
::-webkit-input-placeholder{color:@c9}
.login-content{
	margin: 50px 40px 80px;
	.icon-user{
		vertical-align: bottom;
		width: 18px;
		height: 22px;
		display: inline-block;
		background:url(../images/login-user.png)no-repeat;
		background-size: cover;
	}
	.icon-pass{
		vertical-align: bottom;
		width: 18px;
		height: 22px;
		display: inline-block;
		background:url(../images/login-password.png)no-repeat;
		background-size: cover;
	}
	.icon-eye-open{
		width: 23px;
		height: 14px;
		background: url(../images/eye2.png)no-repeat;
		background-size: cover;
		display: inline-block;
		float: right;
		margin-top:7px;
		&.close{
			background-image:url(../images/eye-close.png);
		}
	}
	.login-item{
		background: #fff;
		padding:17px 0;	
		input{
			padding:0 0 0 10px;
			font-size: 15px;
			width: 85%;
			border:0;
			display:inline-block;
		}
		input.pass{
			width: 70%;
		}
		
	}
}
.bottom-logo{
	width: 296px;
	height: 74px;
	background:url(../images/logo-login.png);
	bottom:20px;
	margin:auto;
}
.login-screen-title{
	margin: 50px auto !important;
}
.page-left-content{
	.block{
		margin-top: 0px;
	}
}
.login-close{
	width: 15px;
	height: 15px;
	background: url(../images/close.png)no-repeat;
	background-size: 15px;
	background-position: center; 
	position: absolute;
	top:15px;
	left:15px;
	padding:10px;
}
.btn{
	height:50px !important;
	line-height:50px !important;
	font-weight:700;
	font-size: 18px;
}
.message-bubble{
	font-size: 13px !important;
	padding-bottom:5px !important;
	line-height: 1.3 !important;
	min-height: 30px !important;
}
.message-wrap{
	margin: 0;
    height: auto;
    cursor: default;
    clear: both;
    position: relative;
    padding: 8px 10px 4px;
    text-align: center;
    .redbag-info1{
    	border-radius: 5px;
	    padding: 3px 8px;
	    background-color: rgba(229,229,234,.4);
	    overflow: hidden;
	    display: inline-block;
	    font-size: 12px;
    }
}
.message-system{
	.message-bubble{
		color: #fff !important;
    	background: #AB47BC !important;
    	background: -webkit-linear-gradient(left, #AB47BC, #5169DE)!important;
        background: -moz-linear-gradient(left, #AB47BC, #5169DE)!important;
        background: -ms-linear-gradient(left, #AB47BC, #5169DE)!important;
        background: -o-linear-gradient(left, #AB47BC, #5169DE)!important;
        background: linear-gradient(to right, #AB47BC, #5169DE)!important;
        filter: progid:DXImageTransform.Microsoft.gradient(startcolorstr=#AB47BC, endcolorstr=#5169DE, gradientType=1);
	}
}
.message-teacher{
	.message-bubble{
		color: #fff !important;
		background: #009688!important;
		background: -webkit-linear-gradient(left, #009688, #396)!important;
		background: -moz-linear-gradient(left, #009688, #396)!important;
		background: -ms-linear-gradient(left, #009688, #396)!important;
		background: -o-linear-gradient(left, #009688, #396)!important;
		background: linear-gradient(to right, #009688, #396)!important;
		filter: progid:DXImageTransform.Microsoft.gradient(startcolorstr=#009688, endcolorstr=#396, gradientType=1);
	}
}
.message-manage{
	.message-bubble{
		background: #F44336!important;
        background: -webkit-linear-gradient(left, #F44336, #F97619)!important;
        background: -moz-linear-gradient(left, #F44336, #F97619)!important;
        background: -ms-linear-gradient(left, #F44336, #F97619)!important;
        background: -o-linear-gradient(left, #F44336, #F97619)!important;
        background: linear-gradient(to right, #F44336, #F97619)!important;
        filter: progid:DXImageTransform.Microsoft.gradient(startcolorstr=#F44336, endcolorstr=#F97619, gradientType=1)!important;
        color: #fff!important;
	}
}